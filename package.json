{
  "name": "chrome-session-quicksave",
  "displayName": "Session Quicksave",
  "description": "A Chrome extension for quickly saving all open tabs inside a window into the browser bookmarks (with some configurability)",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjesuiter/chrome-session-quicksave-manifest-v3.git"
  },
  "author": {
    "name": "Benjamin Jesuiter",
    "email": "bjesuiter@gmail.com"
  },
  "scripts": {
    "dev": "vite build -w --mode development",
    "build": "vite build",
    "prod": "vite build",
    "lint": "eslint --fix",
    "dev:nodemon": "nodemon",
    "build:noemit": "tsc --noEmit && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prettier": "prettier --write .",
    "prefinalize": "bun run build",
    "finalize": "bestzip deploy.zip dist/*"
  },
  "dependencies": {
    "@solid-primitives/storage": "^4.3.1",
    "date-fns": "^4.1.0",
    "delay": "^6.0.0",
    "solid-js": "^1.9.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.30",
    "@eslint/js": "^9.18.0",
    "@iconify/json": "^2.2.299",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/chrome": "0.0.299",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.20",
    "bestzip": "^2.2.1",
    "eslint": "9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "jsdom": "^26.0.0",
    "nodemon": "3.1.9",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.17",
    "tailwindcss-motion": "^1.0.1",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.21.0",
    "unplugin-icons": "^22.0.0",
    "vite": "^6.0.11",
    "vite-plugin-solid": "^2.11.0",
    "vitest": "^3.0.3"
  }
}
